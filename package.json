{
  "name": "nuxt-google-tag-manager",
  "version": "0.1.0",
  "description": "Nuxt.js module for google tag manager, forked from official modules repo to add custom settings",
  "license": "MIT",
  "contributors": [
    {
      "name": "DaxChen <dd@daxchen.com>"
    }
  ],
  "main": "module.js",
  "repository": "https://github.com/DaxChen/nuxt-google-tag-manager",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nuxt test/fixture",
    "lint": "eslint lib test",
    "test": "npm run lint && jest",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "eslintIgnore": [
    "lib/templates/*.*"
  ],
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "dependencies": {},
  "devDependencies": {
    "nuxt": "latest",
    "codecov": "latest",
    "eslint": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-standard": "latest",
    "eslint-plugin-vue": "latest",
    "jest": "latest",
    "jsdom": "latest",
    "standard-version": "latest"
  }
}
